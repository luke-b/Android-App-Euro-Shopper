<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.buyagent"
      android:versionCode="3"
      android:versionName="3.0">
    <uses-sdk android:minSdkVersion="7" />
    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="BuyAgentApplication">
        <activity android:name=".BuyAgentActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="EmptyActivity"></activity>
    <!--  <activity android:name="BuyAgentActivity"></activity> -->
        <activity android:name="ShopInfoActivity"></activity>
        <activity android:name="CommentActivity"></activity>
        <activity android:name="ProductListActivity"></activity>

    </application>
</manifest>